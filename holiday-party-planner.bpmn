    <bpmn:serviceTask id="Activity_1dec2n9" name="Ask ChatGPT for dish suggestions based on dietary restrictions" zeebe:modelerTemplate="io.camunda.connectors.OpenAI.v1" zeebe:modelerTemplateVersion="3" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
